 <div class="col-md-6 container formulario" >

   <%= form_for(:session, url: login_path) do |f| %>
    
    <h2>
    <%= f.label :email %>
    <%= f.email_field :email %>
    </h2>
    <h2>
    <%= f.label :password %>
    <%= f.password_field :password %><br>
    </h2>
    
    <%= f.submit "Log in" %>

<% end %>
